<%- include('partials/header'); -%>

<br><br><br><br>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
	<link rel="preconnect" href="https://fonts.gstatic.com">
<link href="https://fonts.googleapis.com/css2?family=Dancing+Script:wght@500&display=swap" rel="stylesheet">
<link href="https://fonts.googleapis.com/css2?family=Varela+Round&display=swap" rel="stylesheet">

<div class="container" style="margin-top: 10vmin;">
	
<form style="padding: 100px;box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.19);font-family: 'Varela Round', sans-serif;" method="post" action="/Registerinventory">
	<h1 style="font-family: 'Dancing Script', cursive;">Analysis</h1><hr>
  <div class="form-row">
    
    <div class="form-group col-md-6">
        <div class="col-auto">
          <label class="mr-sm-2" for="hospitaltype">&nbsp;Name of Company</label>
          <select class="custom-select mr-sm-2" id="inputhospitaltype" name="inputhospitaltype">
            <option selected disabled>Choose...</option>
            <option value="Government">Government</option>
            <option value="Private">Private</option>
          </select>
        </div>
      </div>

      <div class="form-group col-md-6">
        <div class="col-auto">
          <label class="mr-sm-2" for="hospitaltype">&nbsp;Type of Analysis</label>
          <select class="custom-select mr-sm-2" id="inputhospitaltype" name="inputhospitaltype">
            <option selected disabled>Choose...</option>
            <option value="OHLC">OHLC</option>
            <option value="Candlestick charts">Candlestick charts</option>
            <option value="Colored Bar">Colored Bar</option>
            <option value="Vertex Line">Vertex Line</option>
            <option value="Hollow Candle">Hollow Candle</option>
          </select>
        </div>
      
      
      
    
  </div>
  <div class="form-group col-md-6">
    <label for="inputDOB">&nbsp;From</label>
    <input type="date" class="form-control" id="inputDOB" name="inputDOB" placeholder="DOB" required>
  </div>
  <div class="form-group col-md-6">
    <label for="inputDOB">&nbsp;To</label>
    <input type="date" class="form-control" id="inputDOB" name="inputDOB" placeholder="DOB" required>
  </div>
  

 
  
 
  <div class="row d-flex justify-content-between">
  	<div class="col-md-8">
  		<button type="submit" class="btn btn-primary" style="width: 250px" onclick="myFunction()">Result</button>
  	</div>
  	<div class="col-md-4">
  		<br>
  	</div>
  	
  </div>
</form>

</div>
<script>
  function myFunction() {
    alert("Inventory registration is successful.");
  }
  </script>
  <script type="text/javascript" src="js/end.js"></script>
  <br><br>

<%- include('partials/footer'); -%>